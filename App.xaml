<Application x:Class="DiskBenchmark.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DiskBenchmark"
             StartupUri="Views/Windows/MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="Resources/Styles/PageStyles.xaml" />
                <ResourceDictionary Source="Resources/Styles/ImageStyles.xaml" />
                <ResourceDictionary Source="Resources/Styles/TextStyle.xaml" />
                <ResourceDictionary Source="Resources/Styles/ButtonStyles.xaml" />
                <ResourceDictionary Source="Resources/Styles/ProgressBarStyle.xaml" />
                <ResourceDictionary Source="Resources/Styles/ScrollBarStyle.xaml" />
                <ResourceDictionary Source="Resources/Styles/ComboBoxStyles.xaml" />
                <ResourceDictionary Source="Resources/DataTemplates/DataTemplate.xaml" />
               

            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>

    </Application.Resources>
</Application>
