<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="BorderStyle1" TargetType="Border">
        <Setter Property="Background" Value="{StaticResource BackgroundBrush}"/>
        <Setter Property="CornerRadius" Value="0 0 20 0"/>
    </Style>

    <Style x:Key="BorderStyle2" TargetType="Border">
        <Setter Property="Background" Value="{StaticResource BorderBrush_1}"/>
        <Setter Property="CornerRadius" Value="20"/>
    </Style>

    <Style x:Key="BorderStyle3" TargetType="Border">
        <Setter Property="Background" Value="{StaticResource BorderBrush_2}"/>
        <Setter Property="CornerRadius" Value="0 0 20 0"/>
    </Style>
</ResourceDictionary>