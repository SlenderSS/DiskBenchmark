<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:DiskBenchmark.ViewModels"
                    xmlns:view="clr-namespace:DiskBenchmark.Views">
    
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ConnectedDisksViewModel}">
        <view:ConnectedDisksView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:DiskDetailsViewModel}">
        <view:DiskDetailsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DiskSpeedTestViewModel}">
        <view:DiskSpeedTestView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AboutViewModel}">
        <view:AboutView/>
    </DataTemplate>
    
</ResourceDictionary>