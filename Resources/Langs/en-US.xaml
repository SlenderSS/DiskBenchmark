    <system:String x:Key="CPU">CPU:</system:String>
    <system:String x:Key="GPU">GPU:</system:String>
    <system:String x:Key="BIOS">BIOS:</system:String>
    

    <system:String x:Key="HomeBtn">Home</system:String>
    <system:String x:Key="ViewDisksBtn">View Disks</system:String>
    <system:String x:Key="BenchmarkBtn">Benchmark</system:String>

    <system:String x:Key="ModelDisk">Model:</system:String>
    <system:String x:Key="Serial">Serial:</system:String>
    <system:String x:Key="Type">Type:</system:String>
    <system:String x:Key="Infrastructure">Infrastructure:</system:String>
    <system:String x:Key="Volume">Volume:</system:String>
    <system:String x:Key="Patritions">Patritions:</system:String>
    <system:String x:Key="Signature">Signature:</system:String>
    <system:String x:Key="FirmRev">Firmware revision:</system:String>
